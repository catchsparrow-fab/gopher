@model IEnumerable<Gopher.Models.UserItem>

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Gopher.Models.UserItem item in Model)
            {
                <tr>
                    <td>
                        @item.Id
                    </td>
                    <td>
                        @item.Name
                    </td>
                </tr>
            }
        </tbody>

    </table>
</div>
