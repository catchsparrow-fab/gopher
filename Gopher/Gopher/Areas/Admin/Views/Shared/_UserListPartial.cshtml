@model IEnumerable<Gopher.Model.Domain.User>

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.ActionLink(item.Name, "Edit", "Users", new { id = item.Id }, null)
                    </td>
                </tr>
            }
        </tbody>

    </table>
</div>
